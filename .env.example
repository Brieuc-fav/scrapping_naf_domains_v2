# INSEE API Configuration
SIRENE_API_KEY=your_insee_api_key_here
SIRENE_CLIENT_ID=your_insee_client_id
SIRENE_CLIENT_SECRET=your_insee_client_secret
SIRENE_TOKEN_URL=https://api.insee.fr/token
SIRENE_API_BASE=https://api.insee.fr/api-sirene/3.11

# SerpAPI Configuration (optional)
SERPAPI_KEY=your_serpapi_key_here

# Serper API Configuration - Multiple keys for fallback rotation
# The system will automatically rotate to the next key when quota is exceeded
# You can configure up to 4 API keys (or more by modifying the code)
SERPER_API_KEY_1=your_first_serper_api_key_here
SERPER_API_KEY_2=your_second_serper_api_key_here
SERPER_API_KEY_3=your_third_serper_api_key_here
SERPER_API_KEY_4=your_fourth_serper_api_key_here

# Legacy format (still supported if no numbered keys are provided)
# SERPER_API_KEY=your_serper_api_key_here
